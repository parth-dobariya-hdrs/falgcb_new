# Example environment variables for FastAPI LangGraph Chatbot

# Authentication Settings
SECRET_KEY=your-secret-key-change-in-production
COOKIE_MAX_AGE_ACCESS=3600
COOKIE_MAX_AGE_REFRESH=2592000
JWT_LIFETIME_SECONDS=3600
REFRESH_TOKEN_LIFETIME_SECONDS=2592000

# Clerk Authentication Settings
CLERK_SECRET_KEY=your-clerk-secret-key
CLERK_PUBLISHABLE_KEY=your-clerk-publishable-key
CLERK_WEBHOOK_SECRET=your-clerk-webhook-secret
CLERK_INSTANCE_URL=https://your-instance.clerk.accounts.dev
CLERK_JWT_VERIFICATION_KEY=your-clerk-jwt-verification-key

# Google OAuth Credentials
GOOGLE_OAUTH_CLIENT_ID=your-google-oauth-client-id
GOOGLE_OAUTH_CLIENT_SECRET=your-google-oauth-client-secret

# Database Settings
PSQL_USERNAME=postgres
PSQL_PASSWORD=your-password
PSQL_HOST=localhost
PSQL_PORT=5432
PSQL_DATABASE=chatbot_db
PSQL_SSLMODE=prefer

# Database Pool Settings
DB_POOL_SIZE=50
DB_MAX_OVERFLOW=10

# AI API Keys
GROQ_API_KEY=your-groq-api-key
TAVILY_API_KEY=your-tavily-api-key
GOOGLE_API_KEY=your-google-api-key
